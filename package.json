{
  "name": "digitalcheck-tool-finder",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "homepage": "https://digitalservicebund.github.io/digitalcheck-tool-finder/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preserve": "vite build",
    "start": "vite preview",
    "clean": "rm -rf dist/",
    "test": "jest test",
    "test:e2e": "playwright test",
    "typecheck": "tsc",
    "lint:check": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow '' --summary"
  },
  "dependencies": {
    "@digitalservice4germany/angie": "^1.1.0",
    "@digitalservice4germany/style-dictionary": "^2.0.0"
  },
  "devDependencies": {
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.39.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-playwright": "^0.18.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testing-library": "^6.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.39.0",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
